{
    "name": "notimatica/driver",
    "description": "Web push notifications sending server-side driver. Chrome, Firefox and Safari support.",
    "keywords": ["push", "notifications", "driver", "chrome", "firefox", "safari"],
    "license": "MIT",
    "authors": [
        {
            "name": "Maxim Lanin",
            "email": "max@lanin.me",
            "homepage": "https://blog.lanin.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "Notimatica\\Driver\\": "src/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "maennchen/zipstream-php": "^0.3.1",
        "guzzlehttp/guzzle": "^6.2",
        "league/event": "^2.1",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "^0.9.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Notimatica\\Driver\\Tests\\": "tests/"
        }
    }
}
